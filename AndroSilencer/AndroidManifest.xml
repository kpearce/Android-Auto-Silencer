<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.AndroSilencer"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-sdk android:minSdkVersion="17"/>
    <application android:label="@string/app_name" android:icon="@drawable/ic_launcher">
        <activity android:name="net.kpearce.AndroSilencer.MainActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="net.kpearce.AndroSilencer.LocationTypeActivity"/>
        <activity android:name="net.kpearce.AndroSilencer.WifiScanResultsActivity" android:label="Scan Results"/>
        <activity android:name="net.kpearce.AndroSilencer.NetworkConfigurationActivity" android:label="Networks"/>
        <activity android:name="net.kpearce.AndroSilencer.SaveOrScanActivity" android:label="Saved or Scan"/>
    </application>
</manifest>
